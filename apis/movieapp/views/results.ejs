<h1>Results</h1>
<ul>
    <% data["Search"].forEach(function(movie){ %>
      <li> <strong>
                 <%=movie["Title"]%>
          </strong>-<%=movie["Year"]%> </li>
     <% }) %>  
     
     
     <% for(var i=0;i<data["Search"].length;i++) { %>
      <li> <%=data["Search"][i]["Title"]%> </li>
     <% } %>
 
</ul> 


<a href="/">Search again</a>